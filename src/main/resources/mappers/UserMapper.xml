<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.coming.pet_store_coming_be.dao.UserDAO">

  <select id="getUserByEmail" resultType="boolean" parameterType="String"> 
    select count(1)
    from user
    where email = #{email}
  </select>

</mapper>